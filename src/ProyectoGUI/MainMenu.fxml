<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ProyectoGUI.MainMenuController">
   <children>
      <MenuBar nodeOrientation="RIGHT_TO_LEFT" VBox.vgrow="ALWAYS">
        <menus>
          <Menu mnemonicParsing="false" text="Configuración">
            <items>
              <MenuItem mnemonicParsing="false" text="Usuario" />
            </items>
          </Menu>
        </menus>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </MenuBar>
      <VBox alignment="TOP_CENTER" prefHeight="575.0" prefWidth="400.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="45.0" prefWidth="800.0" spacing="400.0" VBox.vgrow="ALWAYS">
               <children>
                  <ButtonBar prefHeight="33.0" prefWidth="744.0" HBox.hgrow="ALWAYS">
                    <buttons>
                        <Button mnemonicParsing="false" onAction="#cerrarSesion" text="Cerrar Sesión" />
                    </buttons>
                  </ButtonBar>
               </children>
               <VBox.margin>
                  <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
               </VBox.margin>
            </HBox>
            <TableView fx:id="tabla" nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="ALWAYS">
              <columns>
                  <TableColumn fx:id="medicamentoNombreColumn" minWidth="-Infinity" prefWidth="245.0" text="Medicamento" />
                <TableColumn fx:id="medicamentoDosisColumn" minWidth="-Infinity" prefWidth="121.0" text="Dosis" />
                  <TableColumn fx:id="medicamentoVecesDiaColumn" prefWidth="177.0" text="Veces Por Día" />
                  <TableColumn fx:id="medicamentoVecesSemanaColumn" prefWidth="194.0" text="Veces Por Semana" />
              </columns>
               <VBox.margin>
                  <Insets left="30.0" right="30.0" />
               </VBox.margin>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <ButtonBar nodeOrientation="LEFT_TO_RIGHT" prefHeight="65.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
              <buttons>
                  <Button mnemonicParsing="false" text="Añadir medicamento" />
                <Button mnemonicParsing="false" text="Eliminar Medicamento" />
              </buttons>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets right="30.0" />
               </padding>
            </ButtonBar>
         </children>
      </VBox>
   </children>
   <stylesheets>
      <URL value="@../stylesheets/styles.css" />
      <URL value="@../stylesheets/specific.css" />
   </stylesheets>
</VBox>
